<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wy.test.persistence.mapper.LocalizationMapper">

	<resultMap id="localizationMap" type="com.wy.test.core.entity.LocalizationEntity">
		<result property="id" column="id" />
		<result property="property" column="property" />
		<result property="langZh" column="lang_zh" />
		<result property="langEn" column="lang_en" />
		<result property="instId" column="inst_id" />
		<result property="remark" column="remark" />
